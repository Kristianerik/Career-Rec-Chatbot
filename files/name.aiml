<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<!-- name gathering section -->
	<category>
	    <pattern>Start</pattern>
	    <template>Great! Please enter your name in the format "My name is [your name]"</template>
	</category>
	
	<category>
	    <pattern>^ name is *</pattern>
	    <template>
	        <image>https://icons.iconarchive.com/icons/google/noto-emoji-people-bodyparts/256/12051-waving-hand-light-skin-tone-icon.png</image>
	        <think><set name ="name"><star index="2"/></set></think> Hello <get name="name"/>! Are you looking for career advice?
	    </template>
	</category>
	

	
</aiml>